{
  "publisher": "TrippR",
  "repository": {
    "type": "git",
    "url": "https://github.com/RealTrippR/x86-reghover"
  },
  "name": "reghover",
  "displayName": "x86 reghover",
  "description": "Inspect registers by hovering over them, and view pointer contents in a flash",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Debuggers"
  ],
  "activationEvents": [
    "onLanguage:asm",
    "onLanguage:nasm",
    "onLanguage:arm",
    "onLanguage:x86asm",
    "onLanguage:assembly",
    "onLanguage:asm-intel-x86-generic"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "reghover.helloWorld",
        "title": "Hello World"
      }
    ],
    "debuggers": [
      {
        "type": "asmdbg",
        "label": "ASM Debugger",
        "languages": [
          "asm",
          "s",
          "nasm",
          "assembly",
          "asm-intel-x86-generic"
        ]
      }
    ],
    "keybindings": [
      {
        "command": "asmHover.inspectmode",
        "key": "e",
        "when": "editorTextFocus"
      },
      {
        "command": "asmHover.nextmode",
        "key": "q",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}
